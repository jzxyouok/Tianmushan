@implementation TMSBasicTableViewController

- (void)viewDidLoad {
    [super viewDidLoad];
    
    self.tableView.separatorStyle = UITableViewCellSeparatorStyleNone;
    
    self.automaticallyAdjustsScrollViewInsets = NO;
    
    self.navigationItem.leftBarButtonItem = [UIBarButtonItem leftBarbuttonitemWithTitle:@"返回" target:self action:@selector(backItemClick)];

}

- (void)backItemClick
{
    [self.navigationController popViewControllerAnimated:YES];
}


